{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env -- node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -- jest --runInBand",
    "test:unit": "dotenv -e .env.test -- jest --runInBand --config ./jest.unit.config.ts",
    "test:int": "dotenv -e .env.test -- jest --runInBand --config ./jest.int.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "dotenv -e .env.test -- jest --runInBand --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- jest --runInBand --config ./jest.e2e.config.ts",
    "typeorm": "typeorm-ts-node-commonjs -d src/core/config/database/typeorm/ormconfig.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/static": "^7.0.4",
    "@nestjs/common": "^10.3.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.3.2",
    "@nestjs/platform-fastify": "^10.4.1",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/uuid": "^10.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.2",
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.6.0"
  }
}
